#include <stdio.h>

int main(){
    int A[10] = {22,67,31,90,45,56,21,96,100,77};
    int key = 56, found = -1;
    for(int i = 0; i < 10; i++){
        if(A[i] == key){ found = i; break; }
    }
    if(found >= 0) printf("Element %d found at index %d\n", key, found);
    else printf("Element %d not found\n", key);
    return 0;
}
