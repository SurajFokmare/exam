#include <stdio.h>
#define MAX 10

int queue[MAX];
int front = 0, rear = 0; // [front, rear) convention

int isEmpty(){ return front == rear; }
int isFull(){ return rear == MAX; }

void enqueue(int x){
    if(isFull()){ printf("Queue Overflow\n"); return; }
    queue[rear++] = x;
}

int dequeue(){
    if(isEmpty()){ printf("Queue Underflow\n"); return -1; }
    return queue[front++];
}

void display(){
    if(isEmpty()){ printf("[ empty ]\n"); return; }
    printf("Queue (front->rear): ");
    for(int i = front; i < rear; i++) printf("%d ", queue[i]);
    printf("\n");
}

int main(){
    enqueue(1); enqueue(2); enqueue(3);
    printf("After enqueue 1,2,3:\n"); display();
    printf("Dequeued: %d\n", dequeue());
    printf("After dequeuing one:\n"); display();
    return 0;
}
