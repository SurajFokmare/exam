#include <stdio.h>

int main(){
    int A[10] = {22,67,31,90,45,56,21,96,100,77};
    int n = 10;
    for(int i = 0; i < n-1; i++){
        for(int j = 0; j < n-1-i; j++){
            if(A[j] > A[j+1]){
                int t = A[j]; A[j] = A[j+1]; A[j+1] = t;
            }
        }
    }
    printf("Sorted (Bubble): ");
    for(int i=0;i<n;i++) printf("%d ", A[i]);
    printf("\n");
    return 0;
}
