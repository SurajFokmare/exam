#include <stdio.h>

int main(){
    int n ;
    printf("Enter number of rows:");
    scanf("%d",&n);
    for(int i = 1; i <= n; i++){
  
        for(int j = 1; j <= i; j++){
            printf("%d\t", j);
        }
        
        for(int s = 0; s < (n - i); s++) printf("\t");
        
        for(int k = 0; k < i; k++){
            printf("*\t");
        }
        printf("\n");
    }
    return 0;
}
